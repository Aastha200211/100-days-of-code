class Solution:
    def strStr(self, haystack: str, needle: str) -> int:
        n=len(haystack)
        m=len(needle)
        for i in range(n-m+1):
            substring = haystack[i:i +m]  # Take a slice of haystack
            if substring == needle:
                return i  # Found the match, return the index

        return -1  # No match found



    
        
