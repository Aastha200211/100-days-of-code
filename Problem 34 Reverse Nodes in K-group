class Solution:
    def reverseKGroup(self, head: Optional[ListNode], k: int) -> Optional[ListNode]:
        length=0
        node=head
        while node:
            length+=1
            node=node.next
        dummy=ListNode(0)
        dummy.next=head
        prev=dummy

        while length>=k:
            curr=prev.next
            nxt=curr.next
            for i in range(k-1):
                curr.next=nxt.next
                nxt.next=prev.next
                prev.next=nxt
                nxt=curr.next
            prev=curr
            length-=k
        
        return dummy.next
